def func(arr, n, sum): 
    for i in range(0, n - 2):
        for j in range(i + 1, n - 1):
            for k in range(j + 1, n):
                if (arr[i] + arr[j] + arr[k] == sum):
                    print(arr[i], " ", arr[j], " ", arr[k], sep = "")
                    
    return          


arr = [ -1, 0, 1, 2, -1, 4 ]
n = len(arr)
func(arr, n, 0)
